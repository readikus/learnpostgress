  "dependencies": {
    "dotenv": "^15.0.0",
    "log-timestamp": "^0.3.0",
    "pg": "^8.0.2",
    "postgrator": "latest"
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.28.0",
    "husky": "4.3.8",
    "jest": "26.6.3"
